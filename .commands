open-github # Open in GitHub
gp # Git push
git add . && git commit # Git commit all files
ls -la # List all files
git clean -fd # Git clean untracked files
git clean -fdn # Git clean untracked files (dry run)
git status # Git status
git rebase --continue # Git rebase continue
sudo !! # Run last command as root
cd ~/.files # Go to dotfiles
fzc edit # Edit commands
cd $(fzg worktree) # Go to worktree
